FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pisoFoam;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         36.0;

deltaT          2e-04;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      2;

writeFormat     binary;

writePrecision  16;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable false;


functions
{
    #include "probesAveraging"
    #include "probesInitialisation"

    fieldAverage1
    {
        type                fieldAverage;
        functionObjectLibs  (fieldFunctionObjects);
        fields
        (
            U
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }

            p
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }
        );
        writeControl        writeTime;
    }
}



FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    lowerWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    upperWall
    {
        type            symmetry;
    }

    "inlet|outlet"
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
}


